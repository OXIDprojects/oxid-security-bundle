{
  "name": "oxid-community/symfony-security-bundle",
  "type": "oxideshop-module",
  "license": "GPL-3.0-only",
  "require": {
    "php": ">=7.0",
    "lcobucci/jwt": "^3.3",
    "symfony/security-bundle": "~3.1.0"
  },
  "extra": {
    "oxid-kernel-plugin": {
      "symfony/security-bundle": "Symfony\\Bundle\\SecurityBundle\\SecurityBundle",
      "oxid-community/symfony-security-bundle": "OxidCommunity\\SecurityBundle\\OxidCommunitySecurityBundle"
    }
  },
  "autoload": {
    "psr-4": {
